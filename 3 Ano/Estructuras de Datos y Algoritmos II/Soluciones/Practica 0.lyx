\begin_layout Enumerate
Demostrar que 
\begin_inset Formula $f\in\Theta\left(g\right)$
\end_inset

 si y solo si existen constantes 
\begin_inset Formula $c_{1},c_{2}\in\mathbb{R}^{+}$
\end_inset

, 
\begin_inset Formula $n_{0}\in\mathbb{N}$
\end_inset

 tales que:
\begin_inset Formula 
\[
\forall n\geq n_{0}:0\leq c_{1}g\left(n\right)\leq f\left(n\right)\leq c_{2}g\left(n\right)
\]

\end_inset


\end_layout

\begin_deeper
\begin_layout Paragraph
Solución
\end_layout

\begin_layout Itemize
\begin_inset Formula $\boxed{\Rightarrow}$
\end_inset

: Por hipótesis 
\begin_inset Formula $f\in O\left(g\right)$
\end_inset

 y 
\begin_inset Formula $f\in\Omega\left(g\right)$
\end_inset

; y por definición tenemos:
\end_layout

\begin_deeper
\begin_layout Itemize
\begin_inset Formula $\forall n\geq n_{2}:0\leq f\left(n\right)\leq c_{2}g\left(n\right)$
\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset Formula $\forall n\geq n_{1}:0\leq c_{1}g\left(n\right)\leq f\left(n\right)$
\end_inset

.
\end_layout

\begin_layout Standard
Luego tomando 
\begin_inset Formula $n_{0}=\max\left\{ n_{1},n_{2}\right\} $
\end_inset

 valen ambas desigualdades.
\end_layout

\end_deeper
\begin_layout Itemize
\begin_inset Formula $\boxed{\Leftarrow}$
\end_inset

: Por definición tenemos 
\begin_inset Formula $f\in O\left(g\right)$
\end_inset

 y 
\begin_inset Formula $f\in\Omega\left(g\right)$
\end_inset

, luego 
\begin_inset Formula $\text{\ensuremath{f\in\Theta\left(g\right)}}$
\end_inset

.
\end_layout

\end_deeper
\begin_layout Enumerate
Sean 
\begin_inset Formula $f,g:\mathbb{N}\to\mathbb{R}$
\end_inset

 asintoticamente no negativas y 
\begin_inset Formula $h\left(n\right)=f\left(n\right)+g\left(n\right)$
\end_inset

, demostrar que:
\begin_inset Formula 
\[
h\left(n\right)\in\Theta\left(\max\left\{ f\left(n\right),g\left(n\right)\right\} \right)
\]

\end_inset


\end_layout

\begin_deeper
\begin_layout Paragraph
Solución
\end_layout

\begin_layout Standard
Como 
\begin_inset Formula $f$
\end_inset

 y 
\begin_inset Formula $g$
\end_inset

 son asintoticamente no negativas, 
\begin_inset Formula $\text{\ensuremath{\forall n>n_{1}:f\left(n\right)\geq0}}$
\end_inset

 y 
\begin_inset Formula $\forall n>n_{2}:g\left(n\right)\geq0$
\end_inset

; tomando 
\begin_inset Formula $n_{0}=\max\left\{ n_{1},n_{2}\right\} $
\end_inset

 resulta 
\begin_inset Formula $h$
\end_inset

 ser asintoticamente no negativa.
 Ademas, tomando 
\begin_inset Formula $c_{1}=2$
\end_inset

 y 
\begin_inset Formula $c_{2}=1/2$
\end_inset

:
\end_layout

\begin_layout Itemize
\begin_inset Formula $0\leq f\left(n\right)+g\left(n\right)\leq c_{1}\max\left\{ f\left(n\right),g\left(n\right)\right\} \Rightarrow h\in O\left(\max\left\{ f\left(n\right),g\left(n\right)\right\} \right)$
\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset Formula $0\leq c_{2}\max\left\{ f\left(n\right),g\left(n\right)\right\} \leq f\left(n\right)+g\left(n\right)\Rightarrow h\in\Omega\left(\max\left\{ f\left(n\right),g\left(n\right)\right\} \right)$
\end_inset

.
\end_layout

\end_deeper
