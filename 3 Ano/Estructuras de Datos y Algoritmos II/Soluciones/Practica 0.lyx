\end_inset

, luego tomando 
\begin_inset Formula $c=1$
\end_inset

 resultan: 
\end_layout

\begin_deeper
\begin_layout Itemize
\begin_inset Formula $\forall n\geq n_{0}:0\leq f\left(n\right)\leq cf\left(n\right)\Rightarrow f\in O\left(f\right)$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $\forall n\geq n_{0}:0\leq cf\left(n\right)\leq f\left(n\right)\Rightarrow f\in\Omega\left(f\right)$
\end_inset


\end_layout

\end_deeper
\begin_layout Enumerate
Sean 
\begin_inset Formula $f,g/f\in\Theta\left(g\right)$
\end_inset

, luego existen 
\begin_inset Formula $n_{0},c_{1},c_{2}$
\end_inset

 tal que:
\end_layout

\begin_deeper
\begin_layout Standard
\begin_inset Formula 
\[
\forall n\geq n_{0}:0\leq c_{1}g\left(n\right)\leq f\left(n\right)\leq c_{2}g\left(n\right)
\]

\end_inset

luego, dividiendo por 
\begin_inset Formula $c_{1}$
\end_inset

 y por 
\begin_inset Formula $c_{2}$
\end_inset

:
\end_layout

\begin_layout Itemize
\begin_inset Formula $\forall n\geq n_{0}:0\leq g\left(n\right)\leq\frac{1}{c_{1}}f\left(n\right)\Rightarrow g\in O\left(f\right)$
\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset Formula $\forall n\geq n_{0}:0\leq\frac{1}{c_{2}}f\left(n\right)\leq g\left(n\right)\Rightarrow g\in\Omega\left(f\right)$
\end_inset

.
\end_layout

\end_deeper
