\begin_layout Enumerate
\begin_inset Formula $\intop_{-1}^{0}kx^{2}dx=\left.k\frac{x^{3}}{3}\right|_{-1}^{0}=\frac{k0}{3}-\frac{-k}{3}=\frac{k}{3}=1\iff k=3$
\end_inset

.
\end_layout

\begin_layout Enumerate
\begin_inset space ~
\end_inset


\end_layout

\begin_deeper
\begin_layout Itemize
\begin_inset Formula $E\left(X\right)=\intop_{-1}^{0}3x^{2}\cdot xdx=\left.3\frac{x^{4}}{4}\right|_{-1}^{0}=0-\frac{3}{4}=-\frac{3}{4}$
\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset Formula $E\left(X^{2}\right)=\intop_{-1}^{0}3x^{2}\cdot x^{2}dx=\left.3\frac{x^{5}}{5}\right|_{-1}^{0}=0+\frac{3}{5}=\frac{3}{5}$
\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset Formula $V\left(X\right)=E\left(X^{2}\right)-\left[E\left(X\right)\right]^{2}=\frac{3}{5}-\left(-\frac{3}{4}\right)^{2}=\frac{3}{5}-\frac{9}{16}=\frac{3}{80}$
\end_inset

.
\end_layout

\end_deeper
\begin_layout Enumerate
