\begin_layout Enumerate
Simular la ruina del jugador para una inversión inicial de 
\begin_inset Formula $\$2$
\end_inset

, jugando un juego justo.
\end_layout

\begin_deeper
\begin_layout Paragraph
Solución
\end_layout

\begin_layout Standard
\align center
\begin_inset listings
lstparams "language=R,fontfamily=tt"
inline false
status open

\begin_layout Plain Layout

k = 2; n = 5; p = 0.5; q = 1 - p
\end_layout

\begin_layout Plain Layout

gamblersruin <- function() {
\end_layout

\begin_layout Plain Layout

  sim = k
\end_layout

\begin_layout Plain Layout

  while (sum(sim) > 0 && sum(sim) < n + k) {
\end_layout

\begin_layout Plain Layout

    if (rbinom(1, 1, p)) sim = append(sim, 1)
\end_layout

\begin_layout Plain Layout

    else sim = append(sim, -1)
\end_layout

\begin_layout Plain Layout

  }
\end_layout

\begin_layout Plain Layout

  return (sim)
\end_layout

\begin_layout Plain Layout

}
\end_layout

\begin_layout Plain Layout

gamblersruin()
\end_layout

\end_inset


\end_layout

