\begin_layout Standard
\begin_inset Flex Multiple Columns
status open

\begin_layout Itemize
\begin_inset Formula $\overline{a}\left(t\right)=\left(a_{x}\left(t\right),a_{y}\left(t\right)\right)$
\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset Formula $a_{x}\left(t\right)=0$
\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset Formula $a_{y}\left(t\right)=-g$
\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset Formula $\overline{v}\left(t\right)=\left(v_{x}\left(t\right),v_{y}\left(t\right)\right)$
\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset Formula $v_{x}\left(t\right)=v_{0x}$
\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset Formula $v_{y}\left(t\right)=v_{0y}-gt$
\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset Formula $v\left(t\right)=\sqrt{v_{0x}^{2}+\left(v_{0y}-gt\right)^{2}}$
\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset Formula $\overline{p}\left(t\right)=\left(x\left(t\right),y\left(t\right)\right)$
\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset Formula $x\left(t\right)=v_{0x}t$
\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset Formula $y\left(t\right)=v_{0y}t-\frac{1}{2}gt^{2}$
\end_inset

.
\end_layout

\end_inset


\end_layout

\begin_layout Standard
Sea 
\begin_inset Formula $\alpha/v_{y}\left(\alpha\right)=0\iff v_{0y}=g\alpha\iff\alpha=v_{0y}/g$
\end_inset

, luego en dicho tiempo la altura sera maxima.
\end_layout

\begin_layout Standard
Ahora: 
\begin_inset Formula $100=y\left(\alpha\right)=y\left(v_{0y}/g\right)=\frac{v_{0y}^{2}}{g}-\frac{1}{2}\frac{v_{0y}^{2}}{g}=\frac{1}{2}\frac{v_{0y}^{2}}{g}$
\end_inset

 de donde 
\begin_inset Formula $\text{\ensuremath{v_{0y}=\sqrt{200g}}}$
\end_inset

.
\end_layout

\begin_layout Standard
Ademas 
\begin_inset Formula $v_{0x}=v\left(0\right)\cos\left(30\right)\iff v_{0x}=\sqrt{v_{0x}^{2}+200g}\cos\left(30\right)$
\end_inset

 de donde 
\begin_inset Formula $v_{0x}^{2}=\left(v_{0x}^{2}+200g\right)\cos\left(30\right)^{2}\iff v_{0x}^{2}\left[1-\cos\left(30\right)^{2}\right]=200g\cos\left(30\right)^{2}$
\end_inset

.
\end_layout

\begin_layout Standard
Finalmente 
\begin_inset Formula $v_{0x}=\frac{\sqrt{200g}\cos\left(30\right)}{\sin\left(30\right)}=\sqrt{200g}\cot\left(\pi/6\right)$
\end_inset

.
 En resumen:
\end_layout

\begin_layout Itemize
\begin_inset Formula $v_{0x}\approx76,68$
\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset Formula $v_{0y}\approx44,27$
\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset Formula $v_{0}\approx88,54$
\end_inset

.
\end_layout

\end_deeper
\begin_layout Enumerate
Hallar a que angulo 
\begin_inset Formula $\gamma$
\end_inset

 hay que realizar un tiro parabolico para que el alcance y la altura maxima
 sean iguales.
\end_layout

\begin_deeper
\begin_layout Paragraph
Solucion
\end_layout

\begin_layout Standard
Sean 
\begin_inset Formula $\alpha\neq0/y\left(\alpha\right)=0$
\end_inset

 y 
\begin_inset Formula $\beta/v_{y}\left(\beta\right)=0$
\end_inset

, luego el alcance sera 
\begin_inset Formula $x\left(\alpha\right)$
\end_inset

 y la altura maxima 
\begin_inset Formula $y\left(\beta\right)$
\end_inset

 de donde:
\end_layout

\begin_layout Itemize
\begin_inset Formula $\text{\ensuremath{0=v_{0y}\alpha-\frac{1}{2}g\alpha^{2}=\alpha\left(v_{0y}-\frac{1}{2}g\alpha\right)\iff0=v_{0y}-\frac{1}{2}g\alpha\iff\alpha=2v_{0y}/g}}$
\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset Formula $0=v_{0y}-g\beta\iff\beta=v_{0y}/g$
\end_inset

.
\end_layout

\begin_layout Standard
Buscamos que 
\begin_inset Formula $x\left(\alpha\right)=y\left(\beta\right)$
\end_inset

, es decir: 
\begin_inset Formula 
\[
\frac{2v_{0y}v_{0x}}{g}=\frac{v_{0y}^{2}}{g}-\frac{1}{2}\frac{v_{0y}^{2}}{g}=\frac{1}{2}\frac{v_{0y}^{2}}{g}\iff v_{0y}v_{0x}=v_{0y}^{2}\iff v_{0x}=v_{0y}
\]

\end_inset

por lo que 
\begin_inset Formula $\text{\ensuremath{v\left(0\right)\cos\left(\gamma\right)=v\left(0\right)\sin\left(\gamma\right)\iff\cos\left(\gamma\right)=\sin\left(\gamma\right)\iff\gamma=45}}$
\end_inset

.
\end_layout

\end_deeper
