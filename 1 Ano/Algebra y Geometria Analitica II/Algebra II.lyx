\begin_layout Paragraph
Definicion
\end_layout

\begin_layout Standard
Definimos una sucesion 
\begin_inset Formula $u_{1},u_{2},u_{3},\ldots$
\end_inset

 donde el termino n-esimo es en funcion de los anteriores.
\end_layout

\begin_layout Paragraph
Ejemplo
\end_layout

\begin_layout Enumerate
\begin_inset Formula $u_{1}=1$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $u_{n}=2u_{n-1}$
\end_inset

 
\begin_inset Formula $\left(\forall n>1\right)$
\end_inset


\end_layout

\begin_layout Standard
Es decir: 
\begin_inset Formula $u_{1}=1,u_{2}=2,u_{3}=4,u_{4}=8,\ldots$
\end_inset


\end_layout

\begin_layout Subsection
Sumatoria
\end_layout

\begin_layout Subsubsection
Definicion
\end_layout

\begin_layout Standard
Dada una sucesion de numeros reales 
\begin_inset Formula $\left\{ x_{i}:i\in\mathbb{N}\right\} $
\end_inset

 se define la sumatoria desde 
\begin_inset Formula $1$
\end_inset

 hasta 
\begin_inset Formula $n$
\end_inset

 como:
\end_layout

\begin_layout Enumerate
\begin_inset Formula ${\displaystyle \sum_{i=1}^{1}x_{i}=x_{1}}$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula ${\displaystyle \sum_{i=1}^{k+1}x_{i}=\sum_{i=1}^{k}x_{i}+x_{k+1}}$
\end_inset


\end_layout

\begin_layout Subsubsection
Propiedades
\end_layout

\begin_layout Itemize
Aditividad: 
\begin_inset Formula ${\displaystyle \sum_{k=1}^{n}\left(a_{k}+b_{k}\right)=\sum_{k=1}^{n}a_{k}+\sum_{k=1}^{n}b_{k}}$
\end_inset

 
\end_layout

\begin_layout Itemize
Homogeneidad: 
\begin_inset Formula ${\displaystyle \sum_{k=1}^{n}ca_{k}=c\sum_{k=1}^{n}a_{k}}$
\end_inset


\end_layout

\begin_layout Itemize
Telescopica: 
\begin_inset Formula ${\displaystyle \sum_{k=1}^{n}\left(a_{k}-a_{k-1}\right)=a_{n}-a_{0}}$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula ${\displaystyle \sum_{k=1}^{n}c=nc}$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula ${\displaystyle \sum_{i=1}^{m}\sum_{j=1}^{n}a_{i,j}=\sum_{j=1}^{n}\sum_{i=1}^{m}a_{i,j}}$
\end_inset


\end_layout

\begin_layout Section
Conjunto inductivo
\end_layout

\begin_layout Subsection
Definicion
\end_layout

\begin_layout Standard
Un conjunto 
\begin_inset Formula $H\subseteq\mathbb{R}$
\end_inset

 se dice inductivo si:
\end_layout

\begin_layout Itemize
\begin_inset Formula $1\in H$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $k\in H\Rightarrow k+1\in H$
\end_inset


\end_layout

\begin_layout Subsection
Ejemplos
\end_layout

\begin_layout Itemize
\begin_inset Formula $\mathbb{R}$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $\mathbb{N}$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $\mathbb{Z}$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula ${\displaystyle \bigcup_{i=1}^{\infty}\left(i-\epsilon,i+\epsilon\right)}$
\end_inset


\end_layout

\begin_layout Subsection
Lema: interseccion de conjutnos inductivos
\end_layout

\begin_layout Standard
La interseccion arbitraria de conjuntos inductivos es un conjunto inductivo.
\end_layout

\begin_layout Subsubsection
Demostracion
\end_layout

\begin_layout Standard
Sean 
\begin_inset Formula $X_{i}\subseteq\mathbb{R}$
\end_inset

 subconjuntos inductivos con 
\begin_inset Formula $i\in I$
\end_inset

 y sea 
\begin_inset Formula $X={\displaystyle \bigcap_{i\in I}X_{i}}$
\end_inset


\end_layout

\begin_layout Itemize
Como 
\begin_inset Formula $X_{i}$
\end_inset

 son inductivos: 
\begin_inset Formula $1\in X_{i}\Rightarrow1\in X$
\end_inset


\end_layout

\begin_layout Itemize
Sea 
\begin_inset Formula $k\in X\Rightarrow k\in X_{i}\Rightarrow k+1\in X_{i}\Rightarrow k+1\in X$
\end_inset


\begin_inset Newpage pagebreak
\end_inset


\end_layout

\begin_layout Section
Principio de induccion
\end_layout

\begin_layout Subsection
Principio de induccion
\end_layout

\begin_layout Subsubsection
Induccion conjuntista
\end_layout

\begin_layout Paragraph
Enunciado
\end_layout

\begin_layout Standard
Sea 
\begin_inset Formula $H\subseteq\mathbb{N}/1\in\mathbb{N}\land\left(h\in H\Rightarrow h+1\in H\right)$
\end_inset

 entonces 
\begin_inset Formula $H=\mathbb{N}$
\end_inset

.
\end_layout

\begin_layout Paragraph
Demostracion
\end_layout

\begin_layout Standard
Por hipotesis 
\begin_inset Formula $H$
\end_inset

 es inductivo 
\begin_inset Formula $\xRightarrow{\ref{subsec:Definicion de numeros naturales}}\mathbb{N}\subseteq H$
\end_inset

 y como asumimos 
\begin_inset Formula $H\subseteq\mathbb{N}$
\end_inset

 entonces 
\begin_inset Formula $H=\mathbb{N}$
\end_inset

.
\end_layout

\begin_layout Subsubsection
Induccion clasica
\end_layout

\begin_layout Paragraph
Enunciado
\end_layout

\begin_layout Standard
Sea 
\begin_inset Formula $P\left(n\right)$
\end_inset

 una propiedad de 
\begin_inset Formula $n\in\mathbb{N}$
\end_inset

 tal que:
\end_layout

\begin_layout Enumerate
\begin_inset Formula $P\left(1\right)$
\end_inset

 es verdadera.
\end_layout

\begin_layout Enumerate
\begin_inset Formula $P\left(k\right)$
\end_inset

 verdadera 
\begin_inset Formula $\Rightarrow P\left(k+1\right)$
\end_inset

 verdadera.
\end_layout

\begin_layout Standard
entonces 
\begin_inset Formula $P\left(n\right)$
\end_inset

 es verdadera 
\begin_inset Formula $\forall n$
\end_inset

.
\end_layout

\begin_layout Paragraph
Demostracion
\end_layout

\begin_layout Standard
Sea 
\begin_inset Formula $H=\left\{ n\in\mathbb{N}:P\left(n\right)\right\} $
\end_inset

.
\end_layout

\begin_layout Enumerate
Por hipotesis 
\begin_inset Formula $P\left(1\right)$
\end_inset

 es verdadera, luego 
\begin_inset Formula $1\in H$
\end_inset

.
\end_layout

\begin_layout Enumerate
Si 
\begin_inset Formula $k\in H$
\end_inset

 por definicion de 
\begin_inset Formula $H$
\end_inset

, 
\begin_inset Formula $P\left(k\right)$
\end_inset

 es verdadera y por hipotesis 
\begin_inset Formula $P\left(k\right)\Rightarrow P\left(k+1\right)\Rightarrow k+1\in H$
\end_inset


\end_layout

\begin_layout Standard
Hemos demostrado que 
\begin_inset Formula $H$
\end_inset

 es inductivo, luego 
\begin_inset Formula $\mathbb{N}\subseteq H$
\end_inset

 y como 
\begin_inset Formula $H\subseteq\mathbb{N}$
\end_inset

 entonces 
\begin_inset Formula $H=\mathbb{N}$
\end_inset

 
\begin_inset Formula $\therefore P\left(n\right)$
\end_inset

 es verdadera 
\begin_inset Formula $\forall n\in\mathbb{N}=H$
\end_inset

.
\begin_inset Newpage pagebreak
\end_inset


\end_layout

\begin_layout Subsubsection
Induccion desplazada
\end_layout

\begin_layout Paragraph
Enunciado
\end_layout

\begin_layout Standard
Sea 
\begin_inset Formula $P\left(n\right)$
\end_inset

 una propiedad de 
\begin_inset Formula $n\in\mathbb{N}$
\end_inset

 y sea 
\begin_inset Formula $N\in\mathbb{N}$
\end_inset

, si 
\begin_inset Formula $P\left(N\right)$
\end_inset

 es cierta y 
\begin_inset Formula $P\left(k\right)\Rightarrow P\left(k+1\right)$
\end_inset

 para cada 
\begin_inset Formula $k\geq N$
\end_inset

 entonces 
\begin_inset Formula $P\left(n\right)$
\end_inset

 es cierta 
\begin_inset Formula $\forall n\geq N$
\end_inset

.
\end_layout

\begin_layout Paragraph
Demostracion
\end_layout

\begin_layout Standard
Sea 
\begin_inset Formula $H=\left\{ n\in\mathbb{N}:P\left(n+N-1\right)\right\} $
\end_inset

.
\end_layout

\begin_layout Enumerate
Notemos que 
\begin_inset Formula $1\in H$
\end_inset

 pues 
\begin_inset Formula $P\left(1+N-1\right)=P\left(N\right)$
\end_inset

 que es cierta por hipotesis.
\end_layout

\begin_layout Enumerate
Si 
\begin_inset Formula $k\in H$
\end_inset

 entonces por definicion de 
\begin_inset Formula $H$
\end_inset

, 
\begin_inset Formula $P\left(k+N-1\right)$
\end_inset

 es cierta y por hipotesis: 
\begin_inset Formula $P\left(k+N-1\right)\Rightarrow P\left(k+N-1+1\right)=P\left[\left(k+1\right)+N-1\right]$
\end_inset

 luego 
\begin_inset Formula $k+1\in H$
\end_inset

.
\end_layout

\begin_layout Standard
\begin_inset Formula $\therefore H=\mathbb{N}$
\end_inset


\end_layout

\begin_layout Subsubsection
Induccion fuerte conjuntista
\end_layout

\begin_layout Paragraph
Enunciado
\end_layout

\begin_layout Standard
Sea 
\begin_inset Formula $H\subseteq\mathbb{N}/1\in H\land\left\llbracket 1,k\right\rrbracket \subseteq H\Rightarrow\left\llbracket 1,k+1\right\rrbracket \subseteq H$
\end_inset

 entonces 
\begin_inset Formula $H=\mathbb{N}$
\end_inset


\end_layout

\begin_layout Paragraph
Demostracion
\end_layout

\begin_layout Standard
Sea 
\begin_inset Formula $H\subset\mathbb{N}\Rightarrow\overline{H}\neq\emptyset$
\end_inset

 luego por el principio del buen orden 
\begin_inset Formula $\overline{H}$
\end_inset

 tiene primer elemento 
\begin_inset Formula $p>1$
\end_inset

 (pues 
\begin_inset Formula $1\in H$
\end_inset

 por hipotesis), entonces 
\begin_inset Formula $\left\llbracket 1,p-1\right\rrbracket \subseteq H$
\end_inset

 pero por hipotesis si 
\begin_inset Formula $\left\llbracket 1,p-1\right\rrbracket \subseteq H\Rightarrow\left\llbracket 1,p\right\rrbracket \subseteq H$
\end_inset

 por lo que 
\begin_inset Formula $p\in H$
\end_inset

.
 Sin embargo 
\begin_inset Formula $p\in\overline{H}$
\end_inset

 (pues es su primer elemento).
 ¡Contradiccion!
\end_layout

\begin_layout Subsubsection
Induccion fuerte clasica
\end_layout

\begin_layout Paragraph
Enunciado
\end_layout

\begin_layout Standard
Sea 
\begin_inset Formula $P\left(n\right)$
\end_inset

 una propiedad de 
\begin_inset Formula $n\in\mathbb{N}$
\end_inset

 tal que:
\end_layout

\begin_layout Enumerate
\begin_inset Formula $P\left(1\right)$
\end_inset

 es cierta.
\end_layout

\begin_layout Enumerate
Si 
\begin_inset Formula $P\left(1\right),P\left(2\right),P\left(\text{3}\right),\ldots,P\left(k\right)$
\end_inset

 son ciertas, tambien lo es 
\begin_inset Formula $P\left(k+1\right)$
\end_inset

.
\end_layout

\begin_layout Standard
entonces 
\begin_inset Formula $P\left(n\right)$
\end_inset

 es cierta 
\begin_inset Formula $\forall n\in\mathbb{N}$
\end_inset

.
\end_layout

\begin_layout Paragraph
Demostracion
\end_layout

\begin_layout Standard
Sea 
\begin_inset Formula $H=\left\{ n\in\mathbb{N}:P\left(n\right)\right\} $
\end_inset


\end_layout

\begin_layout Enumerate
Por hipotesis 
\begin_inset Formula $P\left(1\right)$
\end_inset

 es cierta luego 
\begin_inset Formula $1\in H$
\end_inset

.
\end_layout

\begin_layout Enumerate
Ademas como 
\begin_inset Formula $P\left(1\right),P\left(2\right),P\left(\text{3}\right),\ldots,P\left(k\right)\Rightarrow P\left(k+1\right)$
\end_inset

 entonces 
\begin_inset Formula $\mbox{\ensuremath{\left\llbracket 1,k+1\right\rrbracket \subseteq H}}.$
\end_inset


\end_layout

\begin_layout Standard
y por el principio de induccion fuerte 
\begin_inset Formula $H=\mathbb{N}$
\end_inset


\end_layout

\begin_layout Section
Numeros naturales
\end_layout

\begin_layout Subsection
Definicion
\begin_inset CommandInset label
LatexCommand label
name "subsec:Definicion de numeros naturales"

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\mathbb{N}$
\end_inset

 es el conjunto de todos los subconjuntos inductivos de 
\begin_inset Formula $\mathbb{R}$
\end_inset

.
\end_layout

\begin_layout Subsection
Propiedades
\begin_inset CommandInset label
LatexCommand label
name "subsec:Propiedades de N"

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $n\in\mathbb{N}\land n\neq1\Rightarrow n-1\in\mathbb{N}$
\end_inset

 o en forma equivalente 
\begin_inset Formula $\exists m\in\mathbb{N}/m+1=n$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $a,b\in\mathbb{N}\Rightarrow a+b\in\mathbb{N}\land ab\in\mathbb{N}$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $a,b\in\mathbb{N}\land a<b\Rightarrow b-a\in\mathbb{N}$
\end_inset


\begin_inset CommandInset label
LatexCommand label
name "enu:Clausura de la resta en N"

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $n\in\mathbb{N}_{0}\land a\in\mathbb{R}/n<a<n+1\Rightarrow a\notin\mathbb{N}$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\mathbb{N}$
\end_inset

 es un conjunto bien ordenado (principio del buen orden).
\end_layout

\begin_layout Subsection
Demostraciones
\end_layout

\begin_layout Enumerate
Sea 
\begin_inset Formula $H=\left\{ 1\right\} \cup\left\{ x\in\mathbb{N}:x=y+1\right\} $
\end_inset

 (para algun 
\begin_inset Formula $y\in\mathbb{N}$
\end_inset

).
\end_layout

\begin_deeper
\begin_layout Enumerate
\begin_inset Formula $1\in H$
\end_inset


\end_layout

\begin_layout Enumerate
Si 
\begin_inset Formula $h\in H$
\end_inset

 entonces 
\begin_inset Formula $h=y+1$
\end_inset

, luego 
\begin_inset Formula $h+1=(y+1)+1$
\end_inset

 por lo que 
\begin_inset Formula $h+1\in H$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $\therefore H=\mathbb{N}$
\end_inset


\end_layout

\end_deeper
\begin_layout Enumerate
Sea 
\begin_inset Formula $b\in\mathbb{N}$
\end_inset

 fijo y sea 
\begin_inset Formula $P\left(n\right):"n+b\in\mathbb{N}"$
\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
\begin_inset Formula $P\left(1\right)$
\end_inset

 es cierta pues 
\begin_inset Formula $b\in\mathbb{N}$
\end_inset

 y 
\begin_inset Formula $\mathbb{N}$
\end_inset

 es inductivo.
\end_layout

\begin_layout Enumerate
Supongamos 
\begin_inset Formula $P\left(k\right)$
\end_inset

 cierta luego 
\begin_inset Formula $k+1+b=\left(k+b\right)+1\in\mathbb{N}$
\end_inset


\begin_inset Newline newline
\end_inset

Finalmente 
\begin_inset Formula $P\left(n\right)$
\end_inset

 es verdadera 
\begin_inset Formula $\forall n,b\in\mathbb{N}$
\end_inset


\end_layout

\end_deeper
\begin_layout Enumerate
Sea 
\begin_inset Formula $P\left(n\right):"n<b\Rightarrow b-n\in\mathbb{N}"$
\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
\begin_inset Formula $P\left(1\right)$
\end_inset

 es cierta por la propiedad 1.
\end_layout

\begin_layout Enumerate
Supongamos 
\begin_inset Formula $P\left(k\right)$
