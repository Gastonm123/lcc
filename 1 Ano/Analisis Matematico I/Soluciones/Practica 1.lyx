\begin_deeper
\begin_layout Standard
\begin_inset Formula 
\[
\begin{array}{cc}
 & \boldsymbol{-\left(-a\right)}\\
= & \left\langle \text{Existencia de neutro}\right\rangle \\
 & -\left(-a\right)+0\\
= & \left\langle \text{Existencia de opuesto}\right\rangle \\
 & -\left(-a\right)+\left(a+\left(-a\right)\right)\\
= & \left\langle \text{Propiedad conmutativa}\right\rangle \\
 & -\left(-a\right)+\left(\left(-a\right)+a\right)\\
= & \left\langle \text{Propiedad asociativa}\right\rangle \\
 & \left(-\left(-a\right)+\left(-a\right)\right)+a\\
= & \left\langle \text{Existencia de neutro}\right\rangle \\
 & 0+a\\
= & \left\langle \text{Existencia de neutro}\right\rangle \\
 & \boldsymbol{a}
\end{array}
\]

\end_inset


\end_layout

\end_deeper
