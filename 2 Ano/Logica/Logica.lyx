\begin_layout Subsection
Substitucion
\end_layout

\begin_layout Standard
Definimos 
\begin_inset Formula $\phi\left[\psi/p_{i}\right]$
\end_inset

 por induccion en 
\begin_inset Formula $\phi$
\end_inset

:
\end_layout

\begin_layout Itemize
\begin_inset Formula $\bot\left[\psi/p_{i}\right]=\bot$
\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset Formula $p_{j}\left[\psi/p_{i}\right]=\begin{cases}
\psi & i=j\\
p_{j} & i\neq j
\end{cases}$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $\left(\phi_{1}\square\phi_{2}\right)\left[\psi/p_{i}\right]=\left(\phi_{1}\left[\psi/p_{i}\right]\square\phi_{2}\left[\psi/p_{i}\right]\right)$
\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset Formula $\left(\neg\phi\right)\left[\psi/p_{i}\right]=\neg\left(\phi\left[\psi/p_{i}\right]\right)$
\end_inset

.
\end_layout

\begin_layout Subsection
Secuencia de formacion
\end_layout

\begin_layout Standard
Una secuencia de formacion 
\begin_inset Formula $\phi_{0},\phi_{1},\ldots,\phi_{n}$
\end_inset

 es una secuencia de formacion para 
\begin_inset Formula $\phi_{n}$
\end_inset

 si y solo si, 
\begin_inset Formula $\forall i\leq n$
\end_inset

 resulta que:
\end_layout

\begin_layout Itemize
\begin_inset Formula $\phi_{i}\in B$
\end_inset

 o bien,
\end_layout

\begin_layout Itemize
\begin_inset Formula $\phi_{i}\equiv\left(\phi_{k}\square\phi_{j}\right)$
\end_inset

 para algunos 
\begin_inset Formula $k,j<i$
\end_inset

 (con 
\begin_inset Formula $\square\in\left\{ \land,\lor,\rightarrow\right\} $
\end_inset

) o bien,
\end_layout

\begin_layout Itemize
\begin_inset Formula $\phi_{i}\equiv\left(\neg\phi_{j}\right)$
\end_inset

 para algun 
\begin_inset Formula $j<i$
\end_inset

.
\end_layout

\begin_layout Subsection
Principio de induccion
\end_layout

\begin_layout Standard
Sea 
\begin_inset Formula $P$
\end_inset

 una propiedad definida sobre 
\begin_inset Formula $PROP$
\end_inset

.
 Cuando:
\end_layout

\begin_layout Itemize
\begin_inset Formula $P\left(\phi\right)$
\end_inset

 es cierta para toda 
\begin_inset Formula $\phi\in B$
\end_inset

 y,
\end_layout

\begin_layout Itemize
si 
\begin_inset Formula $P\left(\phi\right),P\left(\psi\right)$
\end_inset

 son ciertas entonces 
\begin_inset Formula $P\left(\phi\square\psi\right)$
\end_inset

 es cierta y,
\end_layout

\begin_layout Itemize
si 
\begin_inset Formula $P\left(\phi\right)$
\end_inset

 es cierta entonces 
\begin_inset Formula $P\left(\neg\phi\right)$
\end_inset

 es cierta,
\end_layout

\begin_layout Standard
resulta que 
\begin_inset Formula $P$
\end_inset

 es valida para todo elemento de 
\begin_inset Formula $PROP$
\end_inset

.
\end_layout

