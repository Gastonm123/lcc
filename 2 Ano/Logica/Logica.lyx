\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset Formula $Sat\left(\neg\psi_{1}\right)=S-Sat\left(\psi_{1}\right)$
\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset Formula $Sat\left(\psi_{1}\land\psi_{2}\right)=Sat\left(\psi_{1}\right)\cap Sat\left(\psi_{2}\right)$
\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset Formula $Sat\left(\exists\fullmoon\psi\right)=pre_{\exists}\left(Sat\left(\psi\right)\right)$
\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset Formula $Sat\left(\forall\lozenge\psi\right)=inev\left(Sat\left(\psi\right)\right)$
