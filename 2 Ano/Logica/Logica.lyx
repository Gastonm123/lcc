\begin_layout Standard
Sea 
\begin_inset Formula $\Sigma=\left\{ \land,\lor,\rightarrow,\bot,(,),p_{0},p_{1},\ldots\right\} $
\end_inset

 llamaremos:
\end_layout

\begin_layout Itemize
\begin_inset Formula $AT=\left\{ p_{0},p_{1},\ldots\right\} $
\end_inset

 el conjunto de variables proposicionales o proposiciones atomicas.
\end_layout

\begin_layout Itemize
\begin_inset Formula $C=\left\{ \land,\lor,\rightarrow,\bot,\neg\right\} $
\end_inset

 el conjunto de conectivos.
\end_layout

\begin_layout Itemize
\begin_inset Formula $B=AT\cup\left\{ \bot\right\} $
\end_inset

 el conjunto de base inductiva.
\end_layout

\begin_layout Standard
Definimos 
\begin_inset Formula $PROP$
\end_inset

 (el conjunto de proposiciones) como el minimo conjunto tal que:
\end_layout

\begin_layout Enumerate
\begin_inset Formula $B\subset PROP$
\end_inset

.
\end_layout

\begin_layout Enumerate
Si 
\begin_inset Formula $\phi,\psi\in PROP$
\end_inset

 entonces 
\begin_inset Formula $\left(\phi\land\psi\right),\left(\phi\lor\psi\right),\left(\phi\rightarrow\psi\right)\in PROP$
\end_inset

.
\end_layout

\begin_layout Enumerate
Si 
\begin_inset Formula $\phi\in PROP$
\end_inset

 entonces 
\begin_inset Formula $\left(\neg\phi\right)\in PROP$
\end_inset

.
\end_layout

