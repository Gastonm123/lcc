\begin_layout Subsection
Subformulas
\end_layout

\begin_layout Standard
Definimos la funcion 
\begin_inset Formula $subf:PROP\to\mathcal{P}\left(PROP\right)$
\end_inset

 que devuelve todas las subformulas de un elemento de 
\begin_inset Formula $PROP$
\end_inset

 de la siguiente forma:
\end_layout

\begin_layout Itemize
\begin_inset Formula $subf\left(\bot\right)=\left\{ \bot\right\} $
\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset Formula $subf\left(p_{i}\right)=\left\{ p_{i}\right\} $
\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset Formula $subf\left(\neg\phi\right)=\left\{ \neg\phi\right\} \cup subf\left(\phi\right)$
\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset Formula $subf\left(\phi_{1}\square\phi_{2}\right)=\left\{ \phi_{1}\square\phi_{2}\right\} \cup subf\left(\phi_{1}\right)\cup subf\left(\phi_{2}\right)$
\end_inset

, donde 
\begin_inset Formula $\square\in\left\{ \land,\lor,\rightarrow\right\} $
\end_inset

.
\end_layout

\begin_layout Subsection
Conectivos
\end_layout

\begin_layout Standard
Definimos la funcion 
\begin_inset Formula $con:PROP\to\mathbb{N}$
\end_inset

 que devuelve la cantidad de conectivos de un elemento de 
\begin_inset Formula $PROP$
\end_inset

 de la siguiente forma:
\end_layout

\begin_layout Itemize
\begin_inset Formula $con\left(\bot\right)=1$
\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset Formula $con\left(p_{i}\right)=0$
\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset Formula $con\left(\neg\phi\right)=1+con\left(\phi\right)$
\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset Formula $con\left(\phi_{1}\square\phi_{2}\right)=1+con\left(\phi_{1}\right)+con\left(\phi_{2}\right)$
\end_inset

, donde 
\begin_inset Formula $\square\in\left\{ \land,\lor,\rightarrow\right\} $
\end_inset

.
\end_layout

