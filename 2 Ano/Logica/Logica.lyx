\end_inset

: 
\begin_inset Formula $p_{1},p_{2},p_{3},\left(\neg p_{2}\right),\left(\left(\neg p_{2}\right)\rightarrow p_{3}\right),\left(p_{1}\lor\left(\left(\neg p_{2}\right)\rightarrow p_{3}\right)\right)$
\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset Formula $\left(\left(\left(p_{1}\rightarrow p_{2}\right)\rightarrow p_{3}\right)\rightarrow p_{4}\right)$
\end_inset

: 
\begin_inset Formula $p_{1,}p_{2,}p_{3,}p_{4,}\left(p_{1}\rightarrow p_{2}\right),\left(\left(p_{1}\rightarrow p_{2}\right)\rightarrow p_{3}\right),\left(\left(\left(p_{1}\rightarrow p_{2}\right)\rightarrow p_{3}\right)\rightarrow p_{4}\right)$
\end_inset


\end_layout

\begin_layout Subsubsection
Substituciones
\end_layout

\begin_layout Itemize
\begin_inset Formula $\begin{aligned}\left(p_{1}\land p_{0}\right) & \rightarrow\left(p_{0}\rightarrow p_{3}\right)\left[\neg p_{0}\rightarrow p_{3}/p_{0}\right]=\\
 & =\left(p_{1}\land p_{0}\right)\left[\neg p_{0}\rightarrow p_{3}/p_{0}\right]\rightarrow\left(p_{0}\rightarrow p_{3}\right)\left[\neg p_{0}\rightarrow p_{3}/p_{0}\right]=\\
 & =\left(p_{1}\left[\neg p_{0}\rightarrow p_{3}/p_{0}\right]\land p_{0}\left[\neg p_{0}\rightarrow p_{3}/p_{0}\right]\right)\rightarrow\left(p_{0}\left[\neg p_{0}\rightarrow p_{3}/p_{0}\right]\rightarrow p_{3}\left[\neg p_{0}\rightarrow p_{3}/p_{0}\right]\right)\\
 & =\left(p_{1}\land\left(\neg p_{0}\rightarrow p_{3}\right)\right)\rightarrow\left(\left(\neg p_{0}\rightarrow p_{3}\right)\rightarrow p_{3}\right)
\end{aligned}
$
\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset Formula $\begin{aligned}\left(p_{3}\leftrightarrow p_{0}\right) & \lor\left(p_{2}\rightarrow\neg p_{0}\right)\left[\neg p_{0}\rightarrow p_{3}/p_{0}\right]=\\
= & \left(p_{3}\leftrightarrow p_{0}\right)\left[\neg p_{0}\rightarrow p_{3}/p_{0}\right]\lor\left(p_{2}\rightarrow\neg p_{0}\right)\left[\neg p_{0}\rightarrow p_{3}/p_{0}\right]=\\
= & \left(p_{3}\leftrightarrow\left(\neg p_{0}\rightarrow p_{3}\right)\right)\lor\left(p_{2}\rightarrow\neg p_{0}\left[\neg p_{0}\rightarrow p_{3}/p_{0}\right]\right)=\\
= & \left(p_{3}\leftrightarrow\left(\neg p_{0}\rightarrow p_{3}\right)\right)\lor\left(p_{2}\rightarrow\neg\left(\neg p_{0}\rightarrow p_{3}\right)\right)
\end{aligned}
$
\end_inset


