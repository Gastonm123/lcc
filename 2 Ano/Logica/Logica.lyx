\begin_layout Subsubsection
Razonamiento semantico
\end_layout

\begin_layout Itemize
\begin_inset Formula $p\vDash p\lor q$
\end_inset

:
\end_layout

\begin_deeper
\begin_layout Itemize
Sea 
\begin_inset Formula $v$
\end_inset

 una valuacion tal que 
\begin_inset Formula $\left\llbracket p\right\rrbracket _{v}=T$
\end_inset

.
\end_layout

\begin_layout Itemize
Luego 
\begin_inset Formula $\text{\ensuremath{\left\llbracket p\lor q\right\rrbracket _{v}=max\left\{ \left\llbracket p\right\rrbracket _{v},\left\llbracket q\right\rrbracket _{v}\right\} =max\left\{ T,\left\llbracket q\right\rrbracket _{v}\right\} =T}}$
\end_inset

.
\end_layout

\end_deeper
\begin_layout Itemize
Si 
\begin_inset Formula $\underbrace{p\vDash q}_{\left(1\right)}$
\end_inset

 y 
\begin_inset Formula $\underbrace{q\vDash r}_{\left(2\right)}$
\end_inset

 entonces 
\begin_inset Formula $p\vDash r$
\end_inset

:
\end_layout

\begin_deeper
\begin_layout Itemize
Sea 
\begin_inset Formula $v$
\end_inset

 una valuacion tal que 
\begin_inset Formula $\left\llbracket p\right\rrbracket _{v}=T$
\end_inset

.
\end_layout

\begin_layout Itemize
Por 
\begin_inset Formula $\left(1\right)$
\end_inset

 sabemos que 
\begin_inset Formula $\left\llbracket q\right\rrbracket _{v}=T$
\end_inset

.
\end_layout

\begin_layout Itemize
A partir de aqui por 
\begin_inset Formula $\left(2\right)$
\end_inset

 resulta 
\begin_inset Formula $\left\llbracket r\right\rrbracket _{v}=T$
\end_inset

.
\end_layout

\end_deeper
\begin_layout Itemize
Si 
\begin_inset Formula $\vDash p\rightarrow q$
\end_inset

 entonces 
\begin_inset Formula $p\vDash q$
\end_inset

:
\end_layout

\begin_deeper
\begin_layout Itemize
Sea 
\begin_inset Formula $v$
\end_inset

 una valuacion tal que 
\begin_inset Formula $\left\llbracket p\right\rrbracket _{v}=T$
\end_inset

.
\end_layout

\begin_layout Itemize
Por hipotesis 
\begin_inset Formula $\left\llbracket p\rightarrow q\right\rrbracket _{v}=T$
\end_inset

 y por definicion de 
\begin_inset Formula $\rightarrow$
\end_inset

 sabemos que 
\begin_inset Formula $\left\llbracket p\right\rrbracket _{v}\leq\left\llbracket q\right\rrbracket _{v}$
\end_inset

, es decir, 
\begin_inset Formula $T\leq\left\llbracket q\right\rrbracket _{v}$
\end_inset

.
 Por lo tanto 
\begin_inset Formula $\left\llbracket q\right\rrbracket _{v}=T$
\end_inset

.
\end_layout

\end_deeper
\begin_layout Itemize
\begin_inset Formula $\neg\left(p\land q\right)\vDash\neg p\lor\neg q$
\end_inset

:
\end_layout

\begin_deeper
\begin_layout Itemize
Sea 
\begin_inset Formula $v$
\end_inset

 una valuacion tal que 
\begin_inset Formula $\left\llbracket \neg\left(p\land q\right)\right\rrbracket _{v}=T$
\end_inset

.
