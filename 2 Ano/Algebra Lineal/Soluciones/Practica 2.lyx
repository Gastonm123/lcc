\begin_layout Enumerate
Sea 
\begin_inset Formula $\left(V,\mathbb{R},+,\cdot\right)$
\end_inset

 un espacio vectorial y sean 
\begin_inset Formula $U$
\end_inset

 y 
\begin_inset Formula $W$
\end_inset

 subespacios de 
\begin_inset Formula $V$
\end_inset

.
 Probar que
\begin_inset Formula 
\[
U+W=\left\{ v\in V/v=u+w\land u\in U,w\in W\right\} 
\]

\end_inset

es un subespacio de 
\begin_inset Formula $V$
\end_inset

.
\end_layout

\begin_deeper
\begin_layout Paragraph
Solucion
\end_layout

\begin_layout Standard
Por definicion 
\begin_inset Formula $\left(U+W\right)\subseteq V$
\end_inset

.
 Veamos que satisface los axiomas de clausura:
\end_layout

\begin_layout Itemize
Sean 
\begin_inset Formula $x=\left(u_{1}+v_{1}\right)\in\left(U+W\right)$
\end_inset

 y 
\begin_inset Formula $y=\left(u_{2}+v_{2}\right)\in\left(U+W\right)$
\end_inset

 luego 
\begin_inset Formula $x+y=u_{1}+v_{1}+u_{2}+v_{2}=\underbrace{u_{1}+u_{2}}_{\in U}+\underbrace{v_{2}+v_{2}}_{\in V}$
\end_inset

 por lo que 
\begin_inset Formula $\text{\ensuremath{\left(x+y\right)\in\left(U+W\right)}}$
\end_inset

.
\end_layout

\begin_layout Itemize
Sean 
\begin_inset Formula $x=\left(u+v\right)\in\left(U+W\right)$
\end_inset

 y 
\begin_inset Formula $\alpha\in\mathbb{K}$
\end_inset

 luego 
\begin_inset Formula $\alpha x=\underbrace{\alpha u}_{\in U}+\underbrace{\alpha v}_{\in V}$
\end_inset

 por lo que 
\begin_inset Formula $\text{\ensuremath{\left(\alpha x\right)\in\left(U+W\right)}}$
\end_inset

.
\end_layout

\end_deeper
\begin_layout Enumerate
Sean
\begin_inset Formula 
\[
A=\left[\begin{array}{cc}
1 & 0\\
0 & 0
\end{array}\right]\text{ y }B=\left[\begin{array}{cc}
