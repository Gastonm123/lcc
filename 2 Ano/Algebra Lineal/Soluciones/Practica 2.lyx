\end_layout

\begin_layout Enumerate
\begin_inset Formula $\left(-\alpha\cdot\boldsymbol{v}\right)=\alpha\cdot\left(\boldsymbol{-v}\right)$
\end_inset

, donde 
\begin_inset Formula $\boldsymbol{-v}$
\end_inset

 es el opuesto de 
\begin_inset Formula $\boldsymbol{v}$
\end_inset

, 
\begin_inset Formula $\forall v\in V$
\end_inset

.
\end_layout

\begin_layout Enumerate
Si 
\begin_inset Formula $\alpha\cdot\boldsymbol{v}=\boldsymbol{0}$
\end_inset

 entonces 
\begin_inset Formula $\alpha=0$
\end_inset

 o 
\begin_inset Formula $\boldsymbol{v}=\boldsymbol{0}$
\end_inset

.
\end_layout

\begin_layout Paragraph
Soluciones
\end_layout

\begin_layout Enumerate
Por un lado 
\begin_inset Formula $\boldsymbol{0}+\boldsymbol{0'}=\boldsymbol{0}$
\end_inset

 y por otro 
\begin_inset Formula $\boldsymbol{0}+\boldsymbol{0'}=\boldsymbol{0'}$
\end_inset

, por lo que 
\begin_inset Formula $\boldsymbol{0}=\boldsymbol{0'}$
\end_inset

.
\end_layout

\begin_layout Enumerate
En efecto 
\begin_inset Formula $\boldsymbol{\widetilde{x}}=\boldsymbol{\widetilde{x}}+\boldsymbol{0}=\boldsymbol{\widetilde{x}}+\overbrace{\boldsymbol{x}+\boldsymbol{\widetilde{x}'}}^{\boldsymbol{0}}=\underbrace{\boldsymbol{\widetilde{x}}+\boldsymbol{x}}_{\boldsymbol{0}}+\boldsymbol{\widetilde{x}'}=\boldsymbol{\widetilde{x}'}$
\end_inset

.
\end_layout

