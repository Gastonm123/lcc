\begin_layout Enumerate
Verificar la ley del paralelogramo para los vectores 
\begin_inset Formula $u,v\in\mathbb{R}^{n}$
\end_inset

:
\begin_inset Formula 
\[
\left\Vert u+v\right\Vert ^{2}+\left\Vert u-v\right\Vert ^{2}=2\left(\left\Vert u\right\Vert ^{2}+\left\Vert v\right\Vert ^{2}\right)
\]

\end_inset


\end_layout

\begin_deeper
\begin_layout Paragraph
Solucion
\end_layout

\begin_layout Itemize
\begin_inset Formula $u+v=\left(u_{1},\ldots,u_{n}\right)+\left(v_{1},\ldots,v_{n}\right)=\left(u_{1}+v_{1},\ldots,u_{n}+v_{n}\right)$
\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset Formula $\left\Vert u+v\right\Vert ^{2}=\left[u_{1}+v_{1},\ldots,u_{n}+v_{n}\right]\left(u_{1}+v_{1},\ldots,u_{n}+v_{n}\right)=\left(u_{1}+v_{1}\right)^{2}+\ldots+\left(u_{n}+v_{n}\right)^{2}=\underbrace{\left(u_{1}^{2}+2u_{1}v_{1}+v_{1}^{2}\right)+\ldots+\left(u_{n}^{2}+2u_{n}v_{n}+v_{n}^{2}\right)}_{*}$
\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset Formula $\left\Vert u-v\right\Vert ^{2}=\left[u_{1}-v_{1},\ldots,u_{n}-v_{n}\right]\left(u_{1}-v_{1},\ldots,u_{n}-v_{n}\right)=\left(u_{1}-v_{1}\right)^{2}+\ldots+\left(u_{n}-v_{n}\right)^{2}=\underbrace{\left(u_{1}^{2}-2u_{1}v_{1}+v_{1}^{2}\right)+\ldots+\left(u_{n}^{2}-2u_{n}v_{n}+v_{n}^{2}\right)}_{**}$
\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset Formula $*+**=2\left(u_{1}^{2}+v_{1}^{2}\right)+\ldots+2\left(u_{n}^{2}+v_{n}^{2}\right)=2\left[\left(u_{1}^{2}+v_{1}^{2}\right)+\ldots+\left(u_{n}^{2}+v_{n}^{2}\right)\right]=2\left[\left(u_{1}^{2}+\ldots+u_{n}^{2}\right)+\left(v_{1}^{2}+\ldots+v_{n}^{2}\right)\right]=2\left(\left\Vert u\right\Vert ^{2}+\left\Vert v\right\Vert ^{2}\right)$
\end_inset

.
\end_layout

\end_deeper
