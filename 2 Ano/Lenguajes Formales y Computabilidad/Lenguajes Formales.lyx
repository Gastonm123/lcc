\begin_layout Standard
\begin_inset Formula 
\[
\begin{aligned} & \overline{f}^{19}\left[2^{0}3^{0}5^{11101111}7^{1}\right]=\overline{f}^{18}\left[T_{f}\left(0,0,11101111,1\right)\right]=\\
