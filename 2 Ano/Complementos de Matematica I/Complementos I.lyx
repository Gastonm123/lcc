\begin_inset listings
lstparams "language=Python"
inline false
status open

\begin_layout Plain Layout

kruskal(G):
\end_layout

\begin_layout Plain Layout

	E' = [];
\end_layout

\begin_layout Plain Layout

	while len(E') - 1 != len(V):
\end_layout

\begin_layout Plain Layout

		min = infinity;
\end_layout

\begin_layout Plain Layout

		for each e in E:
\end_layout

