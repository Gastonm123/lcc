Algoritmos
\end_layout

\begin_layout Subsection
Busqueda a lo ancho
\end_layout

\begin_layout Paragraph
Entrada
\end_layout

\begin_layout Standard
Un grafo 
\begin_inset Formula $G=\left(V,E\right)$
\end_inset

 con vertices ordenados 
\begin_inset Formula $v_{1},\ldots,v_{n}$
\end_inset

.
\end_layout

\begin_layout Paragraph
Salida
\end_layout

\begin_layout Standard
Un arbol de expansion.
\end_layout

\begin_layout Paragraph
Algoritmo
\end_layout

\begin_layout Standard
 
\end_layout

\begin_layout Standard
\begin_inset listings
lstparams "language=Python"
inline false
status open

\begin_layout Plain Layout

bfs(G):
\end_layout

\begin_layout Plain Layout

	S[0] = v1;
\end_layout

\begin_layout Plain Layout

	V' = [v1];
\end_layout

\begin_layout Plain Layout

	E' = [];
\end_layout

\begin_layout Plain Layout

	while len(V) != len(E') + 1: # faltan aristas
\end_layout

\begin_layout Plain Layout

		for each x in S:
\end_layout

\begin_layout Plain Layout

			for each y in V - V':
\end_layout

\begin_layout Plain Layout

				if {x,y} in E:
\end_layout

\begin_layout Plain Layout

					E' = E' ++ [{x, y}];
\end_layout

\begin_layout Plain Layout

					V' = V' ++ [y];
\end_layout

\begin_layout Plain Layout

		S = sort(sons(S));
\end_layout

\begin_layout Plain Layout

	return (V', E');
\end_layout

\end_inset


\end_layout

\begin_layout Subsection
Busqueda en profundidad
\end_layout

\begin_layout Paragraph
Entrada
\end_layout

\begin_layout Standard
Un grafo 
\begin_inset Formula $G=\left(V,E\right)$
\end_inset

 con vertices ordenados 
\begin_inset Formula $v_{1},\ldots,v_{n}$
\end_inset

.
\end_layout

\begin_layout Paragraph
Salida
\end_layout

\begin_layout Standard
Un arbol de expansion.
\begin_inset Newpage pagebreak
\end_inset


\end_layout

\begin_layout Paragraph
Algoritmo
\end_layout

\begin_layout Standard
 
\end_layout

\begin_layout Standard
\begin_inset listings
lstparams "language=Python"
inline false
status open

\begin_layout Plain Layout

dfs(G):
\end_layout

\begin_layout Plain Layout

	w = v1;
\end_layout

\begin_layout Plain Layout

	V' = [v1];
\end_layout

\begin_layout Plain Layout

	E' = [];
\end_layout

\begin_layout Plain Layout

	while len(V) != len(E') + 1: # faltan aristas
\end_layout

\begin_layout Plain Layout

		while exist {w, vk} | vk not in V':
\end_layout

\begin_layout Plain Layout

			k = minimum such that {w, vk} not in V';
\end_layout

\begin_layout Plain Layout

			E' = E' ++ [{w, vk}];
\end_layout

\begin_layout Plain Layout

			V' = V' ++ [vk];
\end_layout

\begin_layout Plain Layout

			w = vk;
\end_layout

\begin_layout Plain Layout

		w = fahter(w); # retroceso
\end_layout

\begin_layout Plain Layout

	return (V', E');
\end_layout

\end_inset


\end_layout

\begin_layout Subsection
Algoritmo de Prim
\end_layout

\begin_layout Paragraph
Entrada
\end_layout

\begin_layout Standard
Un grafo ponderado 
\begin_inset Formula $G=\left(V,E,w\right)$
\end_inset

 con vertices 
\begin_inset Formula $v_{1},\ldots,v_{n}$
\end_inset

.
\end_layout

\begin_layout Paragraph
Salida
\end_layout

\begin_layout Standard
Un arbol de expansion minimo.
\end_layout

\begin_layout Paragraph
Algoritmo
\end_layout

\begin_layout Standard
 
\end_layout

\begin_layout Standard
\begin_inset listings
lstparams "language=Python"
inline false
status open

\begin_layout Plain Layout

prim(G):
\end_layout

\begin_layout Plain Layout

	for i in [2..n]: v[i] = false;
\end_layout

\begin_layout Plain Layout

	v[1] = true;
\end_layout

\begin_layout Plain Layout

	E' = [];
\end_layout

\begin_layout Plain Layout

	for i in [1..n-1]: # se agregan n-1 aristas
\end_layout

\begin_layout Plain Layout

		min = infinity;
\end_layout

\begin_layout Plain Layout

		for j in [1..n]:
\end_layout

\begin_layout Plain Layout

			if v[j]:
\end_layout

\begin_layout Plain Layout

				for k in [1..n]:
\end_layout

\begin_layout Plain Layout

					if not (v[k] && w(j,k) < min):
\end_layout

\begin_layout Plain Layout

						add_vertex = k;
\end_layout

\begin_layout Plain Layout

						e = {j, k};
\end_layout

\begin_layout Plain Layout

						min = w(j,k);
\end_layout

\begin_layout Plain Layout

		v[add_vertex] = true;
\end_layout

\begin_layout Plain Layout

		E' = E' ++ [e];
\end_layout

\begin_layout Plain Layout

	return (V,E');
\end_layout

\end_inset


\end_layout

\begin_layout Subsection
Algoritmo de Kruskal
