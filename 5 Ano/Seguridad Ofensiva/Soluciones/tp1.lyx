def egcd(a, b):                                                        
                    
\end_layout

\begin_layout Plain Layout

    if a == 0:                                                         
                    
\end_layout

\begin_layout Plain Layout

        return (b, 0, 1)                                               
                    
\end_layout

\begin_layout Plain Layout

    else:                                                              
                    
\end_layout

\begin_layout Plain Layout

        g, y, x = egcd(b % a, a)                                       
                    
\end_layout

\begin_layout Plain Layout

        return (g, x - (b // a) * y, y)                                
                    
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

def modinv(a, m):
\end_layout

\begin_layout Plain Layout

    g, x, y = egcd(a, m)
