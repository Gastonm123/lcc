\begin_layout Plain Layout

    if g != 1:
\end_layout

\begin_layout Plain Layout

        raise Exception('modular inverse does not exist')
\end_layout

\begin_layout Plain Layout

    else:
\end_layout

\begin_layout Plain Layout

        return x % m
\end_layout

