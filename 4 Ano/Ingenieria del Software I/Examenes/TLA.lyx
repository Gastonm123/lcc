\begin_layout Enumerate
Describa formalmente con cierto detalle la semántica de una especificación
 TLA de la forma:
\color purple

\begin_inset Formula 
\[
Init\land\square\left[Op_{1}\lor Op_{2}\right]_{v}\land WF_{v}\left(Op_{1}\right)
\]

\end_inset


\end_layout

\begin_deeper
\begin_layout Paragraph
Solución
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
 & \left\langle s_{0},s_{1},\ldots\right\rangle \left\llbracket Init\land\square\left[Op_{1}\lor Op_{2}\right]_{v}\land WF_{v}\left(Op_{1}\right)\right\rrbracket \\
\equiv & \left\langle s_{0},s_{1},\ldots\right\rangle \left\llbracket Init\land\square\left(Op_{1}\lor Op_{2}\lor v'=v\right)\land WF_{v}\left(Op_{1}\right)\right\rrbracket \\
\equiv & \land\left\langle s_{0},s_{1},\ldots\right\rangle \left\llbracket Init\right\rrbracket \\
 & \land\left\langle s_{0},s_{1},\ldots\right\rangle \left\llbracket \square\left(Op_{1}\lor Op_{2}\lor v'=v\right)\right\rrbracket \\
 & \land\left\langle s_{0},s_{1},\ldots\right\rangle \left\llbracket WF_{v}\left(Op_{1}\right)\right\rrbracket \\
\equiv & \land s_{0}\left\llbracket Init\right\rrbracket \\
 & \land\forall i\in\mathbb{N}:s_{i}\left\llbracket Op_{1}\lor Op_{2}\lor v'=v\right\rrbracket s_{i+1}\\
 & \land\left\langle s_{0},s_{1},\ldots\right\rangle \left\llbracket WF_{v}\left(Op_{1}\right)\right\rrbracket \\
\equiv & \land Init\left[\forall v\in Vars:v\leftarrow s_{0}\left(v\right)\right]\\
 & \land\forall i\in\mathbb{N}:s_{i}\left\llbracket Op_{1}\right\rrbracket s_{i+1}\lor s_{i}\left\llbracket Op_{2}\right\rrbracket s_{i+1}\lor s_{i}\left\llbracket v'=v\right\rrbracket s_{i+1}\\
 & \land\left\langle s_{0},s_{1},\ldots\right\rangle \left\llbracket WF_{v}\left(Op_{1}\right)\right\rrbracket \\
\equiv & \land Init\left[\forall v\in Vars:v\leftarrow s_{0}\left(v\right)\right]\\
 & \begin{array}{ccc}
\land\forall i\in\mathbb{N}: & \lor & Op_{1}\left[\forall v\in Vars:v\leftarrow s_{i}\left(v\right),v'\leftarrow s_{i+1}\left(v\right)\right]\\
 & \lor & Op_{2}\left[\forall v\in Vars:v\leftarrow s_{i}\left(v\right),v'\leftarrow s_{i+1}\left(v\right)\right]\\
 & \lor & s_{i}\left(v\right)=s_{i+1}\left(v\right)
\end{array}\\
 & \land\left\langle s_{0},s_{1},\ldots\right\rangle \left\llbracket WF_{v}\left(Op_{1}\right)\right\rrbracket 
\end{align*}

\end_inset


\end_layout

\end_deeper
