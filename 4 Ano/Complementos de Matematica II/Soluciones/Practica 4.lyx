\begin_layout Enumerate
Mostrar que una flecha de una categoría puede ser monomorfismo y epimorfismo
 y no isomorfismo.
\begin_inset Newpage pagebreak
\end_inset


\end_layout

\begin_deeper
\begin_layout Paragraph
Solución
\end_layout

\begin_layout Standard
Consideremos en 
\begin_inset Formula $Mon$
\end_inset

 el morfismo 
\begin_inset Formula $f:\mathbb{N}_{0}\to\mathbb{Z}$
\end_inset

 tal que 
\begin_inset Formula $f\left(n\right)=n$
\end_inset

.
 Observemos que 
\begin_inset Formula $n_{1}\neq n_{2}\Rightarrow f\left(n_{1}\right)\neq f\left(n_{2}\right)$
\end_inset

.
\end_layout

\begin_layout Itemize
Monomorfismo: Sean 
\begin_inset Formula $g,h:M\to\mathbb{N}_{0}$
\end_inset

 morfismos tales que 
\begin_inset Formula $\text{\ensuremath{f\circ g=f\circ h}}$
\end_inset

 y supongamos existe 
\begin_inset Formula $m\in M$
\end_inset

 tal que 
\begin_inset Formula $g\left(m\right)\neq h\left(m\right)$
\end_inset

, luego:
\begin_inset Formula 
\[
f\circ g\left(m\right)=f\circ h\left(m\right)\iff f\left(g\left(m\right)\right)=f\left(h\left(m\right)\right)
\]

\end_inset

lo cual es una contradicción.
 Por lo tanto 
\begin_inset Formula $f$
\end_inset

 es monomorfismo.
\end_layout

\begin_layout Itemize
Epimorfismo: Sean 
\begin_inset Formula $g,h:\mathbb{Z}\to M$
\end_inset

 morfismos tales que 
\begin_inset Formula $g\circ f=h\circ f$
\end_inset

 y supongamos existe 
\begin_inset Formula $z\in\mathbb{Z}$
\end_inset

 tal que 
\begin_inset Formula $g\left(z\right)\neq h\left(z\right)$
\end_inset

, luego:
\begin_inset Formula 
\[
g\circ f\left(z\right)=h\circ f\left(z\right)\iff g\left(f\left(z\right)\right)=h\left(f\left(z\right)\right)
\]

\end_inset


\end_layout

\begin_deeper
\begin_layout Standard
lo cual es una contradicción.
 Por lo tanto 
\begin_inset Formula $f$
\end_inset

 es epimorfismo.
\end_layout

\end_deeper
\begin_layout Itemize
Isomorfismo: Supongamos que existe 
\begin_inset Formula $f^{-1}:\mathbb{Z}\to\mathbb{N}_{0}$
\end_inset

 tal que 
\begin_inset Formula $\text{\ensuremath{f\circ f^{-1}=id_{\mathbb{Z}}}}$
\end_inset

 y 
\begin_inset Formula $f^{-1}\circ f=id_{\mathbb{N}_{0}}.$
\end_inset

 En particular para 
\begin_inset Formula $n\geq0$
\end_inset

 sabemos que 
\begin_inset Formula $\text{\ensuremath{f^{-1}\left(n\right)=n}}$
\end_inset

, luego:
\begin_inset Formula 
\[
0=f^{-1}\left(0\right)=f^{-1}\left(-n+n\right)=f\left(-n\right)^{-1}+f\left(n\right)^{-1}=f\left(-n\right)^{-1}+n
\]

\end_inset

por lo que 
\begin_inset Formula $f\left(-n\right)^{-1}=-n$
\end_inset

 lo cual es absurdo ya que 
\begin_inset Formula $-n\notin\mathbb{N}_{0}$
\end_inset

.
 Por lo tanto 
\begin_inset Formula $f$
\end_inset

 no es isomorfismo.
\end_layout

\end_deeper
