\begin_layout Itemize
\begin_inset Formula $ob\,\mathcal{P}=P$
\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset Formula $mor\,\mathcal{P}=\leq$
\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset Formula $dom\left(a,b\right)=a$
\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset Formula $cod\left(a,b\right)=b$
\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset Formula $\left(b,c\right)\circ\left(a,b\right)=\left(a,c\right)$
\end_inset

.
\end_layout

\end_inset


\end_layout

\begin_layout Standard
Luego:
\end_layout

\begin_layout Itemize
Para cada objeto 
\begin_inset Formula $a$
\end_inset

 por reflexividad 
\begin_inset Formula $\left(a,a\right)\in\leq$
\end_inset

 por lo que 
\begin_inset Formula $\text{\ensuremath{\left(a,a\right)\in mor\,\mathcal{P}}}$
\end_inset

; a este morfismo lo llamaremos 
\begin_inset Formula $id_{a}$
\end_inset

 resultando: 
\end_layout

\begin_deeper
\begin_layout Itemize
\begin_inset Formula $\left(a,b\right)\circ id_{a}=\left(a,b\right)\circ\left(a,a\right)=\left(a,b\right)$
\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset Formula $id_{a}\circ\left(c,a\right)=\left(a,a\right)\circ\left(c,a\right)=\left(c,a\right)$
\end_inset

.
