\end_layout

\begin_deeper
\begin_layout Itemize
\begin_inset Formula $\forall f\in Hom\left(A,B\right):f\circ id_{A}=f$
\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset Formula $\forall f\in Hom\left(B,A\right):id_{A}\circ f=f$
\end_inset

.
\end_layout

\end_deeper
\end_deeper
