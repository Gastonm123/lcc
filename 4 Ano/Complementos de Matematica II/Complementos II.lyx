\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset Formula $\left(id_{C},id_{D}\right)\circ_{\times}\left(f,g\right)=\left(id_{C}\circ f,id_{D}\circ g\right)=\left(f,g\right)$
\end_inset

.
\end_layout

\end_deeper
\begin_layout Itemize

\emph on
Asociatividad
